      speaker: {fileID: 11400000, guid: 39f4b84492ef33e448f442402e1cdff0, type: 2}
      emotion: happy
      sentenceText: But I'm telling ya, we had something special.
      isChoice: 0
      choices: []
    - lineId: 
      singleUse: 0
      hasCondition: 0
      conditions: []
      speaker: {fileID: 11400000, guid: 308ab4fb6b9614449adcbd703525ff0e, type: 2}
      emotion: straightforward
      sentenceText: Yeah, for the three hours you were there?
      isChoice: 0
      choices: []
