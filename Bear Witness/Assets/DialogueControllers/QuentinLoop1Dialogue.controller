      hasCondition: 0
      conditions: []
      speaker: {fileID: 11400000, guid: 39f4b84492ef33e448f442402e1cdff0, type: 2}
      emotion: angry
      sentenceText: 'Hey, I spent <i>days</i> with her, altogether. It''s not my
