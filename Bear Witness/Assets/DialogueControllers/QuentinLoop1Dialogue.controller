      speaker: {fileID: 11400000, guid: 308ab4fb6b9614449adcbd703525ff0e, type: 2}
      emotion: skeptical
      sentenceText: Why are you so desperate for news from Belcharis?
      isChoice: 0
      choices: []
    - lineId: 
      singleUse: 0
      hasCondition: 0
      conditions: []
      speaker: {fileID: 11400000, guid: 308ab4fb6b9614449adcbd703525ff0e, type: 2}
      emotion: straightforward
      sentenceText: You thinking about a certain someone?
      isChoice: 0
      choices: []
    - lineId: 
      singleUse: 0
      hasCondition: 0
      conditions: []
      speaker: {fileID: 11400000, guid: 39f4b84492ef33e448f442402e1cdff0, type: 2}
      emotion: guilty
      sentenceText: Hey, come on man, I'm just asking.
      isChoice: 0
