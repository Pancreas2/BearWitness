      isChoice: 0
      choices: []
    - lineId: 
      singleUse: 0
      hasCondition: 0
      conditions: []
      speaker: {fileID: 11400000, guid: 308ab4fb6b9614449adcbd703525ff0e, type: 2}
      emotion: skeptical
      sentenceText: Whatever you say, Q.
